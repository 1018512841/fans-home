<div class=" alert alert-default alert-dismissible fade in">

  <button type="button" class="close close_detail_btn" data-dismiss="alert" aria-label="Close">
    <span aria-hidden="true">&times;</span>
  </button>

  <a class="show_detail_btn close" data-toggle="tooltip"
     title="<%= I18n.t "btn_show_tooltip"  %>" href="<%= life_posts_path %>" >
    <i class="icon-eject"></i>
  </a>

  <h4 class="alert-heading">Life is like a piece of cake</h4>

  <div class="span12">
    <div class="container-fluid">
      <div class="row-fluid">
        <%= render "life_posts/life_dashboard" %>

      </div>
    </div>
  </div>
</div>

